<?xml version="1.0"?>
<rss version="2.0">
	<channel>
		<title>{@site}</title>
		<link>/rss</link>
		<description>{@site} - RSS Feed</description>
		<F3:repeat index="{@item}" group="{@entries}">
			<item>
				<title>{@item.tc}</title>
				<link>/#{@item.tc}</link>
				<description>{substr(@item.ad,0,300)} ...</description>
			</item>
		</F3:repeat>
	</channel>
</rss>
